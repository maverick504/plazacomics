{
    "name": "cretueusebiu/laravel-nuxt",
    "description": "A Laravel-Nuxt starter project template.",
    "keywords": ["laravel", "nuxt", "vue", "spa"],
    "license": "MIT",
    "type": "project",
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/EmilianoJaimes/laravel-follow"
      }
    ],
    "require": {
        "php": "^7.1.3",
        "awssat/laravel-visits": "^1.5",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.4",
        "laravel/framework": "5.6.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelista/comments": "^3.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "overtrue/laravel-follow": "^1.1.13",
        "predis/predis": "^1.1",
        "spatie/laravel-cors": "^1.0",
        "tymon/jwt-auth": "^1.0.0-rc.2",
        "vinkla/hashids": "^4.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^3.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Constants/Common.php",
            "app/Constants/Messages.php",
            "app/Helpers/Common.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
              "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate",
            "@php artisan jwt:secret --force"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
